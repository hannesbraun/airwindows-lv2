@prefix doap:	<http://usefulinc.com/ns/doap#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix lv2:	<http://lv2plug.in/ns/lv2core#> .
@prefix pprops:	<http://lv2plug.in/ns/ext/port-props#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix units:	<http://lv2plug.in/ns/extensions/units#> .

<https://hannesbraun.net/ns/lv2/airwindows/mv>
	a lv2:Plugin ,
		lv2:ReverbPlugin ,
		doap:Project ;
	doap:name "MV" ;
	rdfs:comment "Dual-mono reverb based on BitShiftGain and the old Midiverbs" ;
	doap:license <https://opensource.org/licenses/MIT> ;
	doap:maintainer [
		foaf:name "Hannes Braun" ;
		foaf:homepage <https://hannesbraun.net/>
	] ;

	lv2:minorVersion 2 ;
	lv2:microVersion 0 ;

	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "inL" ;
		lv2:name "In L"
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "inR" ;
		lv2:name "In R"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "outL" ;
		lv2:name "Out L"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "outR" ;
		lv2:name "Out R"
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:symbol "depth" ;
		lv2:name "Depth" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 5 ;
		lv2:symbol "bright" ;
		lv2:name "Bright" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 6 ;
		lv2:symbol "regen" ;
		lv2:name "Regen" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 7 ;
		lv2:symbol "output" ;
		lv2:name "Output" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		units:unit units:coef ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 8 ;
		lv2:symbol "drywet" ;
		lv2:name "Dry/Wet" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] .
