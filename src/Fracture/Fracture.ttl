@prefix doap:	<http://usefulinc.com/ns/doap#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix lv2:	<http://lv2plug.in/ns/lv2core#> .
@prefix pprops:	<http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .

<https://hannesbraun.net/ns/lv2/airwindows/fracture>
	a lv2:Plugin ,
		lv2:WaveshaperPlugin ,
		doap:Project ;
	doap:name "Fracture" ;
	rdfs:comment "Frequency multiplier/waveshaper with a soft disruption to the sound" ;
	doap:license <https://opensource.org/licenses/MIT> ;
	doap:maintainer [
		foaf:name "Hannes Braun" ;
		foaf:homepage <https://hannesbraun.net/>
	] ;

	lv2:minorVersion 2 ;
	lv2:microVersion 0 ;

	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "inL" ;
		lv2:name "In L"
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "inR" ;
		lv2:name "In R"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "outL" ;
		lv2:name "Out L"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "outR" ;
		lv2:name "Out R"
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:symbol "drive" ;
		lv2:name "Drive" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 4.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 5 ;
		lv2:symbol "fracture" ;
		lv2:name "Fracture" ;
		lv2:default 2.5 ;
		lv2:minimum 1.0 ;
		lv2:maximum 3.999 ;
		lv2:scalePoint [
			rdfs:label "1" ;
			rdf:value 1
		] , [
			rdfs:label "2" ;
			rdf:value 2.5
		] , [
			rdfs:label "3" ;
			rdf:value 3.999
		] ;
		lv2:portProperty lv2:enumeration ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 6 ;
		lv2:symbol "outputlevel" ;
		lv2:name "Output Level" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 7 ;
		lv2:symbol "drywet" ;
		lv2:name "Dry/Wet" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty pprops:hasStrictBounds
	] .
